        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".Resultat">

    <TextView
            android:text="@string/resultat"
            android:layout_width="409dp"
            android:layout_height="wrap_content" android:id="@+id/resClair" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="@string/resultat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/resChiffre"/>

    <Button
            android:text="@string/buttonRetour"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button2" tools:layout_editor_absoluteY="108dp"
            android:onClick="returnOnClick"
            tools:layout_editor_absoluteX="1dp" tools:ignore="MissingConstraints"/>
</LinearLayout>